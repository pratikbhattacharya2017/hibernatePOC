package com.topgear.assignment.hibernate;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name="Student")
public class Student {

	@Id
	int id;
	
	@Column
	String firstName;
	@Column
	String lastName;
	@Column
	String email;
	
	
}
